def sum_even(x):
    x = 10
    sum = 0
    for i in range(1, x):
        if i % 2 == 0:
            sum += i
    return sum

print(sum_even(10))

def calculate_bmi(kg, m):
    return kg/((m*m))
result = calculate_bmi(10,20)
print(result)

def count_down_up(n):
    for i in range(n, 1, -1):
        print(i, end= " ")
    for i in range(1, n+1, 1):
        print(i, end = " ")
        
count_down_up(5)
